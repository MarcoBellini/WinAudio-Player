        MENUITEM "Add Files...",                ID_FILE_ADDFILES
        MENUITEM SEPARATOR
        MENUITEM "Settings",                    ID_FILE_SETTINGS
        MENUITEM SEPARATOR
        MENUITEM "Close\tALT-F4",               ID_FILE_CLOSE
    END
