    BEGIN
        MENUITEM "Play\tALT-Q",                 ID_PLAYBACK_PLAY
        MENUITEM "Pause\tALT-W",                ID_PLAYBACK_PAUSE
        MENUITEM "Stop\tALT-E",                 ID_PLAYBACK_STOP, CHECKED
        MENUITEM SEPARATOR
